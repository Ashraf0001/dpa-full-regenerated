[
  {
    "name": "positive_amounts",
    "column": "amount",
    "rule_type": "sql",
    "expression": "amount <= 0",
    "message": "Amount must be positive",
    "severity": "error"
  },
  {
    "name": "valid_countries",
    "column": "country",
    "rule_type": "sql",
    "expression": "country NOT IN ('US', 'IT', 'DE', 'ES', 'NL', 'GB', 'FR')",
    "message": "Invalid country code",
    "severity": "error"
  },
  {
    "name": "amount_range",
    "column": "amount",
    "rule_type": "range",
    "expression": "0.01,1000.0",
    "message": "Amount must be between 0.01 and 1000.0",
    "severity": "warning"
  },
  {
    "name": "valid_channels",
    "column": "channel",
    "rule_type": "sql",
    "expression": "channel NOT IN ('web', 'app', 'pos')",
    "message": "Invalid channel type",
    "severity": "error"
  },
  {
    "name": "future_timestamps",
    "column": "timestamp",
    "rule_type": "sql",
    "expression": "timestamp > 1750000000",
    "message": "Timestamp is in the future",
    "severity": "warning"
  }
]

